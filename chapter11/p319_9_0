#include "head.h"//a special answer not widespread 

void turn1();
int main()
{
	turn1();

	return 0;
}

void turn1()
{
	char a[6] = "abcde";
	printf("%s\n", a);
	printf("%d\n", strlen(a));
	
	
	char b[6];
	int na = 0;
	int nb = 6 -2- na;

	for (na = 0; na < 5; na++)
	{
		nb = 6-2 - na;
		b[nb] = a[na];
		printf("%c %c\n", a[na], b[nb]);
	}
	printf("%s\n", a);
	b[5] = '\0';
	printf("%s\n", b);
	

}
